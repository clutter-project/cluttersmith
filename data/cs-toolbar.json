{
  "id":"cs-dialog-toolbar",
  "name":"toolbar",
  "type":"MxBoxLayout",
  "style-class":"parasite",
  "spacing":2,
  "children":[
    {
      "id":"cs-mode",
      "width":230,
      "type":"MxComboBox"
    },
    {
      "type":"MxButton",
      "name":"cs-copy",
      "tooltip-text":"Copy",
      "signals":[
        {
          "name":"clicked",
          "handler":"cs_copy"
        }
      ]
    },
    {
      "type":"MxButton",
      "name":"cs-cut",
      "tooltip-text":"Cut",
      "signals":[
        {
          "name":"clicked",
          "handler":"cs_cut"
        }
      ]
    },
    {
      "type":"MxButton",
      "name":"cs-paste",
      "tooltip-text":"Paste",
      "signals":[
        {
          "name":"clicked",
          "handler":"cs_paste"
        }
      ]
    },
    {
      "type":"MxButton",
      "name":"cs-duplicate",
      "label":"duplicate",
      "tooltip-text":"Duplicate",
      "signals":[
        {
          "name":"clicked",
          "handler":"cs_duplicate"
        }
      ]
    },
    {
      "type":"MxButton",
      "name":"cs-remove",
      "tooltip-text":"Remove",
      "signals":[
        {
          "name":"clicked",
          "handler":"cs_remove"
        }
      ]
    },
    {
      "type":"MxButton",
      "name":"cs-raise-top",
      "tooltip-text":"Send to front",
      "signals":[
        {
          "name":"clicked",
          "handler":"cs_raise_top"
        }
      ]
    },
    {
      "type":"MxButton",
      "name":"cs-raise",
      "tooltip-text":"Raise",
      "signals":[
        {
          "name":"clicked",
          "handler":"cs_raise"
        }
      ]
    },
    {
      "type":"MxButton",
      "name":"cs-lower",
      "tooltip-text":"Lower",
      "signals":[
        {
          "name":"clicked",
          "handler":"cs_lower"
        }
      ]
    },
    {
      "type":"MxButton",
      "name":"cs-bottom",
      "tooltip-text":"Send to bottom",
      "signals":[
        {
          "name":"clicked",
          "handler":"cs_lower_bottom"
        }
      ]
    },
    {
      "type":"MxButton",
      "name":"cs-reset-size",
      "tooltip-text":"Reset size",
      "signals":[
        {
          "name":"clicked",
          "handler":"cs_reset_size"
        }
      ]
    }
  ]
}
